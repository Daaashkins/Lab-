1.#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int A,B;
    cout<<"Введите A, B";
    cin>>A>>B;
    for (int i = A; i <= B; i ++) {
        for (int j=0;j<i;j++){
             cout <<i;
        }
        cout<<"\n";
    }
}


2.#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int A,B;
    cout<<"Введите A, B";
    cin>>A>>B;
    while(A>B){
        A=A-B;
    }
    cout<<"Незанятая часть равна "<<A;
    
}

3.#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int N, K=0, S=0;
    cout<<"Введите N";
    cin>>N;
    while(S<=N){
        K++;
        S=K+S;
    
    }
    cout<<K<<" "<<S;
    
}

4.#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int K=0, P;
    float S=1000;
    cout<<"Введите P";
    cin>>P;
    while(S<=1100){
        K++;
        S=S/100*P+S;
    
    }
    cout<<"Количество месяцев "<<K<<"\nСумма "<<S;
    
}

5.#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int A,B;
    cout<<"Введите A,B";
    cin>>A>>B;
    while(A!=B){
        if (A>B){
            A=A-B;
        }
        else{
            B=B-A;
        }
    
    }
    cout<<"НОД "<<A;
    
}

6.#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int N;
    cout<<"Введите N";
    cin>>N;
    int A1=1, A2=1, f=0,k=2;
    while(f<N){
        k++;
        f=A1+A2;
        A2=A1;
        A1=f;
      
    }
    cout<<"K= "<<k;
}

