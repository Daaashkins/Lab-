1.#include <iostream>
#include <cmath>
using namespace std;

double PowerA3(double A) {
	return A*A*A;
}

int main() {
	double x, y;
	cout<<"Введите x, y";
	cin>>x>>y;
	y = PowerA3(x);
		cout << "B = A^3 = " << y << endl;
	}
	
2.#include <iostream>
#include <cmath>
using namespace std;

int sign(float x){
   if (x<0){ 
       return -1;
   }    
   if (x==0){ 
       return 0;
       
   }    
   if (x>0)
   { return 1;
       
   }
}

int main() {
	double a, b;
	cout<<"Введите a, b";
	cin>>a>>b;
		cout << "Sign(A)+Sign(B) =" << sign(a)+sign(b) << endl;
	}
