1.#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int N;
    cout<<"Введите значение секунд";
    cin>>N;
    int A=N/60;
    cout<<"Количество секунд, прошедших с начала последней минуты "<<N-A*60;
    
} 


2. #include <iostream>
#include <cmath>
using namespace std;

int main(){
    int N;
    cout<<"Введите номер дня";
    cin>>N;
    if (N>1 && N<366){
    cout<<"номер дня недели для K-го дня года "<<N%7;
    }
    else{
    cout<<"Введено неправильное значение";
    }
    
}

3. #include <iostream>
#include <cmath>
using namespace std;

int main(){
    int K, N;
    cout<<"Введите K, N";
    cin>>K>>N;
    if (N>0 && N<366 && N>0 && N<8){
    cout<<"номер дня недели для K-го дня года "<<K%N;
    }
    else{
    cout<<"Введено неправильное значение";
    }
    
}

4.#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int A,B,C;
    cout<<"Введите A,B,C";
    cin>>A>>B>>C;
    int Z=(A*B)/(C*C);
    cout<<"Количество квадратов "<<A<<"\nНезанятая площадь"<<A*B-C*C*Z;
 
}

5.#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int A;
    cout<<"Введите A";
    cin>>A;
    
    cout<<"номер столетия "<<A/100+1;
    
   
    
}
